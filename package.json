{
  "name": "pasteorreplaceimage",
  "displayName": "Paste or replace image",
  "description": "Paste or replace image (including gifs) from clipboard",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "replace",
    "paste",
    "image",
    "clipboard",
    "png", "jpg", "bmp", "gif"
  ],
  "repository": {"type": "git", "url": "https://github.com/goohan/pasteorreplaceimage"},
  "icon": "assets/icon.png",
  "license": "MIT",
  "publisher": "goohan",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pasteorreplaceimage.replaceimagefromclipboard",
        "title": "Replace image from clipboard"
      },
      {
        "command": "pasteorreplaceimage.pasteimagefromclipboard",
        "title": "Paste image"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "pasteorreplaceimage.replaceimagefromclipboard",
          "when": "resourceExtname == .png || resourceExtname == .jpg || resourceExtname == .bmp || resourceExtname == .gif"
        },
        {
          "command": "pasteorreplaceimage.pasteimagefromclipboard",
          "when": "explorerResourceIsFolder"
        }
      ],
      "explorer/context": [
        {
          "command": "pasteorreplaceimage.replaceimagefromclipboard",
          "group": "7_modification",
          "when": "resourceExtname == .png || resourceExtname == .jpg || resourceExtname == .bmp || resourceExtname == .gif"
        },
        {
          "command": "pasteorreplaceimage.pasteimagefromclipboard",
          "group": "5_cutcopypaste",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  }
}
